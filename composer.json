{
    "name": "natsenack/wp-pdf-builder-pro",
    "description": "WordPress PDF Builder Pro",
    "type": "wordpress-plugin",
    "license": "GPL-3.0",
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "": "src/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test:coverage": "phpunit --coverage-html coverage",
        "cs": "phpcs",
        "cs:fix": "phpcbf"
    },
    "config": {
        "optimize-autoloader": true,
        "prefer-stable": true,
        "disable-tls": true
    }
}
