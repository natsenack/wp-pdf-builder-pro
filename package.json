{
  "name": "wp-pdf-builder-pro",
  "version": "1.1.0",
  "description": "WordPress PDF Builder Pro - Frontend Assets",
  "scripts": {
    "build": "webpack --config=dev/config/build/webpack.config.js --mode=production",
    "build-prod": "webpack --config=dev/config/build/webpack.config.js --mode=production",
    "dev": "webpack --config=dev/config/build/webpack.config.js --mode=development",
    "watch": "webpack --config=dev/config/build/webpack.config.js --mode=development --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:integration": "node -e \"console.log('Running integration tests...'); import('./assets/js/pdf-canvas-tests.js').then(() => console.log('Tests loaded successfully')).catch(e => console.error('Test load failed:', e))\""
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
