{
  "name": "natsenack/wp-pdf-builder-pro",
  "description": "WordPress PDF Builder Pro",
  "type": "wordpress-plugin",
  "license": "GPL-3.0",
  "require": {
    "php": ">=7.4",
    "dompdf/dompdf": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "squizlabs/php_codesniffer": "^3.0",
    "php-stubs/wordpress-stubs": "*"
  },
  "autoload": {
    "psr-4": {
      "PDF_Builder\\": "src/",
      "PDF_Builder_Pro\\": "src/"
    },
    "files": [
      "src/Core/PDF_Builder_Path_Validator.php",
      "src/Core/PDF_Builder_Rate_Limiter.php",
      "src/Core/PDF_Builder_Security_Validator.php"
    ]
  },
  "scripts": {
    "test": "phpunit",
    "test:coverage": "phpunit --coverage-html coverage",
    "cs": "phpcs",
    "cs:fix": "phpcbf"
  },
  "config": {
    "optimize-autoloader": true,
    "prefer-stable": true,
    "disable-tls": true
  }
}
